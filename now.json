{
  "version" : 2,
  "name": "ae-18-2020",
  "builds":[
    {
      "src":"*.html",
      "use":"@now/static"
    },
    {
      "src":"*.css",
      "use":"@now/static"
    },
    {
      "src":"assets/*.*",
      "use":"@now/static"
    }
  ],
  "routes":[
    {
      "src":"/(.*)",
      "dest":"$1"
    }
  ]
}
